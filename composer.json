{
    "name": "dikki/claude-sdk-docs",
    "description": "Documentation for PHP Claude SDK",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "dikki/claude-sdk": "^0.0.2",
        "dikki/dotenv": "^2.0",
        "dikki/markdown": "^1.0",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/container": "^4.2",
        "league/plates": "^3.6",
        "league/route": "^6.2",
        "rakibtg/sleekdb": "^2.15",
        "tracy/tracy": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}